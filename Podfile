platform :ios, '9.0'

def pods
  inhibit_all_warnings!
  
  pod 'MGArchitecture', :git => 'https://github.com/HoaPham98/MGArchitecture', :branch => 'hoapq'
  pod 'NSObject+Rx', '4.4'
  pod 'Then', '2.4'
  pod 'Reusable', '4.1.0'
  pod 'MJRefresh', '~> 3.2', :modular_headers => true
end

target 'MGLoadMore' do
  pods
end
